---
import Layout from "../../../layouts/Layout.astro";
import Productcat from "../../../components/productcat.astro";
import Backmenu from "../../../components/Backmenu.astro";
import Projectinproduct from "../../../components/Projectinproduct.astro";

const aqitems = [
    {
        imgSrc: "/images/13_ongridvsoffgrid-Cover.jpg",
        imgAlt: "DA377: Quốc Oai - Hà Nội",
        heading: "DA377: Quốc Oai - Hà Nội",
        buttonLink: "https://youtu.be/JXQ3A8eFYmc?si=VyPKSXhA0KrHZoCK",
    },
    {
        imgSrc: "/images/16_cocheHD-Cover.jpg",
        imgAlt: "DA393: Thái Bình",
        heading: "DA393: Thái Bình",
        buttonLink: "https://youtu.be/-eBkEvXXRaw?si=Mhp38M-qpUUfrhIV",
    },
    {
        imgSrc: "/images/7_Sosanh2he-Cover.jpg",
        imgAlt: "DA208: Ứng Hoà - Hà Nội",
        heading: "DA208: Ứng Hoà - Hà Nội",
        buttonLink: "https://youtu.be/iIu3RhKuI_8?si=Ac7Y54OIvjeXlZ8i",
    },
];


const combopage = [
    {
        title: "Combo On - Grid | 10.2kW | 1 pha",
        subtitle: "Sản lượng trung bình: 900 - 1100 số/tháng",
        price: "86.800.000",
        image: "/images/Ongrid-1-pha.jpg",
    },
];

const products = [
    {
        title: "Tấm pin mặt trời 600 Wp",
        soluong: "17",
        dvt: "tấm",
        logo: "/images/brand-ja-solar-logo.svg",
        image: "/images/solar-jasolar.png",
    },
    {
        title: "Inverter on-grid 10kW",
        soluong: "01",
        dvt: "bộ",

        logo: "/images/brand-invt-logo.png",
        image: "/images/inverter-invt.png",
    },

    {
        title: "Hệ khung giá đỡ nhôm cao cấp",
        soluong: "01",
        dvt: "bộ",

        logo: "/images/logo-solarmax.png",
        image: "https://image.made-in-china.com/202f0j00QqncubeGhToZ/Wholesaler-Aluminium-Frame-PV-Panel-Stand-Adjustable-Front-Rear-Leg-Mount-Flat-Roof-Solar-Panel-System-Mounting-Bracket.webp",
    },
    {
        title: "Bộ tủ điện năng lượng mặt trời",
        soluong: "01",
        dvt: "bộ",
        logo: "/images/logo-solarmax.png",
        image: "/images/bo-tu-dien-solarmax.png",
    },
];
---

<Layout title=`${combopage[0].title}` `>
    <div class="bg-slate-800 z-50 absolute w-full">
        <Backmenu />

        <div class="mx-auto px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl lg:px-8">
            <!-- Product -->

            <div
                class="relative p-4 rounded-lg lg:grid lg:grid-cols-7 lg:grid-rows-1 lg:gap-x-8 lg:gap-y-10 xl:gap-x-16 bg-white"
            >
                <button
                    type="button"
                    class="absolute -top-3 -right-3 -m-2.5 inline-flex items-center justify-center rounded-md p-2.5 text-red-500"
                    onclick="history.back()"
                >
                    <svg
                        xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 24 24"
                        fill="currentColor"
                        class="size-10"
                    >
                        <path
                            fill-rule="evenodd"
                            d="M12 2.25c-5.385 0-9.75 4.365-9.75 9.75s4.365 9.75 9.75 9.75 9.75-4.365 9.75-9.75S17.385 2.25 12 2.25Zm-1.72 6.97a.75.75 0 1 0-1.06 1.06L10.94 12l-1.72 1.72a.75.75 0 1 0 1.06 1.06L12 13.06l1.72 1.72a.75.75 0 1 0 1.06-1.06L13.06 12l1.72-1.72a.75.75 0 1 0-1.06-1.06L12 10.94l-1.72-1.72Z"
                            clip-rule="evenodd"></path>
                    </svg>
                </button>
                <!-- Product image -->
                <div class="lg:col-span-4 lg:row-end-1">
                    <div
                        class="aspect-h-3 aspect-w-4 overflow-hidden rounded-lg bg-gray-100"
                    >
                        <img
                            transition:name="ongrid2"
                            src=`${combopage[0].image}`
                            alt="Sample of 30 icons with friendly and fun details in outline, filled, and brand color styles."
                            class="object-cover object-center"
                        />
                    </div>
                </div>

                <!-- Product details -->
                <div
                    class="mx-auto mt-1 max-w-2xl sm:mt-16 lg:col-span-3 lg:row-span-2 lg:row-end-2 lg:mt-0 lg:max-w-none"
                >
                    <div class="flex flex-col-reverse">
                        <div class="mt-4">
                            <h1
                                class="text-2xl font-bold tracking-tight text-gray-900 sm:text-3xl"
                            >
                                {combopage[0].title}
                            </h1>

                            <h2 id="information-heading" class="sr-only">
                                Product information
                            </h2>
                            <p class="mt-2 text-md text-gray-500">
                                {combopage[0].subtitle}
                            </p>
                        </div>

                        <div>
                            <h3 class="sr-only">Reviews</h3>
                            <div class="flex items-center">
                                <!-- Active: "text-yellow-400", Default: "text-gray-300" -->
                            </div>
                            <p class="sr-only">4 out of 5 stars</p>
                        </div>
                    </div>
                    <ul role="list" class="divide-y divide-gray-100">
                        {
                            products.map((product) => (
                                <li class="relative py-5 hover:bg-gray-50">
                                    <div class=" sm:px-2 lg:px-6">
                                        <div class="mx-auto flex max-w-4xl justify-between gap-x-6">
                                            <div class="flex min-w-0 gap-x-4">
                                                <img
                                                    class="h-12 w-12 whitespace-nowrap flex-none bg-white"
                                                    src={product.image}
                                                    alt={product.title}
                                                />
                                                <div class="min-w-0 flex-auto">
                                                    <p class="text-md font-semibold leading-6 text-gray-900">
                                                        {product.title}
                                                    </p>
                                                    <img
                                                        class="flex-none w-20 shadow-lg p-2 rounded-lg"
                                                        src={product.logo}
                                                        alt={product.title}
                                                    />
                                                </div>
                                            </div>
                                            <div class="flex shrink-0 items-center gap-x-4">
                                                <div class=" sm:flex sm:flex-col sm:items-end ">
                                                    <div class="rounded-md bg-red-100 leading-4  border-l-2 shadow-lg text-red-600	 px-2 py-1 text-center  ">
                                                        <span class="font-semibold text-2xl">
                                                            {product.soluong}
                                                        </span>
                                                        <br />

                                                        <span>
                                                            {product.dvt}
                                                        </span>
                                                    </div>
                                                </div>
                                                {/* <svg
                                                    class="h-5 w-5 flex-none text-gray-400"
                                                    viewBox="0 0 20 20"
                                                    fill="currentColor"
                                                    aria-hidden="true"
                                                >
                                                    <path
                                                        fill-rule="evenodd"
                                                        d="M7.21 14.77a.75.75 0 01.02-1.06L11.168 10 7.23 6.29a.75.75 0 111.04-1.08l4.5 4.25a.75.75 0 010 1.08l-4.5 4.25a.75.75 0 01-1.06-.02z"
                                                        clip-rule="evenodd"
                                                    />
                                                </svg> */}
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            ))
                        }
                    </ul>

                    <div
                        class="mt-10 grid grid-cols-1 gap-x-6 gap-y-4 sm:grid-cols-2"
                    >
                        <div
                            class="flex w-full items-center justify-center rounded-md bg-red-50 px-8 py-3 text-2xl fon font-bold text-red-700 border-2 border-red-700 hover:bg-red-100 focus:outline-none focus:ring-2 focus:ring-red-500 focus:ring-offset-2 focus:ring-offset-gray-50"
                        >
                            {combopage[0].price}<sup>đ</sup>
                        </div>
                        <a href="https://zalo.me/0964920242" target="_blank" >
                            <button
                                type="button"
                                class="flex w-full items-center justify-center rounded-md border border-transparent bg-red-600 px-8 py-3 text-2xl font-medium text-white hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-offset-2 focus:ring-offset-gray-50"
                                >Chat Zalo</button
                            >
                        </a>

                        <Productcat />

                        <Projectinproduct aqitems={aqitems} />

                        <div
                            class="hidden mt-10 border-t border-gray-200 pt-10"
                        >
                            <h3 class="text-sm font-medium text-gray-900">
                                Tuỳ chọn nâng cấp
                            </h3>
                            <div class="prose prose-sm mt-4 text-gray-500">
                                <ul role="list">
                                    <li>Pin PV 2 mặt kính</li>
                                    <li>
                                        Compatible with Figma, Sketch, and Adobe
                                        XD
                                    </li>
                                    <li>Drawn on 24 x 24 pixel grid</li>
                                </ul>
                            </div>
                        </div>

                        <div
                            class="hidden mt-10 border-t border-gray-200 pt-10"
                        >
                            <h3 class="text-sm font-medium text-gray-900">
                                License
                            </h3>
                            <p class="mt-4 text-sm text-gray-500">
                                For personal and professional use. You cannot
                                resell or redistribute these icons in their
                                original or modified state. <a
                                    href="#"
                                    class="font-medium text-red-600 hover:text-red-500"
                                    >Read full license</a
                                >
                            </p>
                        </div>

                        <div
                            class="hidden mt-10 border-t border-gray-200 pt-10"
                        >
                            <h3 class="text-sm font-medium text-gray-900">
                                Share
                            </h3>
                            <ul
                                role="list"
                                class="mt-4 flex items-center space-x-6"
                            >
                                <li>
                                    <a
                                        href="#"
                                        class="flex h-6 w-6 items-center justify-center text-gray-400 hover:text-gray-500"
                                    >
                                        <span class="sr-only"
                                            >Share on Facebook</span
                                        >
                                        <svg
                                            class="h-5 w-5"
                                            fill="currentColor"
                                            viewBox="0 0 20 20"
                                            aria-hidden="true"
                                        >
                                            <path
                                                fill-rule="evenodd"
                                                d="M20 10c0-5.523-4.477-10-10-10S0 4.477 0 10c0 4.991 3.657 9.128 8.438 9.878v-6.987h-2.54V10h2.54V7.797c0-2.506 1.492-3.89 3.777-3.89 1.094 0 2.238.195 2.238.195v2.46h-1.26c-1.243 0-1.63.771-1.63 1.562V10h2.773l-.443 2.89h-2.33v6.988C16.343 19.128 20 14.991 20 10z"
                                                clip-rule="evenodd"></path>
                                        </svg>
                                    </a>
                                </li>
                                <li>
                                    <a
                                        href="#"
                                        class="flex h-6 w-6 items-center justify-center text-gray-400 hover:text-gray-500"
                                    >
                                        <span class="sr-only"
                                            >Share on Instagram</span
                                        >
                                        <svg
                                            class="h-6 w-6"
                                            fill="currentColor"
                                            viewBox="0 0 24 24"
                                            aria-hidden="true"
                                        >
                                            <path
                                                fill-rule="evenodd"
                                                d="M12.315 2c2.43 0 2.784.013 3.808.06 1.064.049 1.791.218 2.427.465a4.902 4.902 0 011.772 1.153 4.902 4.902 0 011.153 1.772c.247.636.416 1.363.465 2.427.048 1.067.06 1.407.06 4.123v.08c0 2.643-.012 2.987-.06 4.043-.049 1.064-.218 1.791-.465 2.427a4.902 4.902 0 01-1.153 1.772 4.902 4.902 0 01-1.772 1.153c-.636.247-1.363.416-2.427.465-1.067.048-1.407.06-4.123.06h-.08c-2.643 0-2.987-.012-4.043-.06-1.064-.049-1.791-.218-2.427-.465a4.902 4.902 0 01-1.772-1.153 4.902 4.902 0 01-1.153-1.772c-.247-.636-.416-1.363-.465-2.427-.047-1.024-.06-1.379-.06-3.808v-.63c0-2.43.013-2.784.06-3.808.049-1.064.218-1.791.465-2.427a4.902 4.902 0 011.153-1.772A4.902 4.902 0 015.45 2.525c.636-.247 1.363-.416 2.427-.465C8.901 2.013 9.256 2 11.685 2h.63zm-.081 1.802h-.468c-2.456 0-2.784.011-3.807.058-.975.045-1.504.207-1.857.344-.467.182-.8.398-1.15.748-.35.35-.566.683-.748 1.15-.137.353-.3.882-.344 1.857-.047 1.023-.058 1.351-.058 3.807v.468c0 2.456.011 2.784.058 3.807.045.975.207 1.504.344 1.857.182.466.399.8.748 1.15.35.35.683.566 1.15.748.353.137.882.3 1.857.344 1.054.048 1.37.058 4.041.058h.08c2.597 0 2.917-.01 3.96-.058.976-.045 1.505-.207 1.858-.344.466-.182.8-.398 1.15-.748.35-.35.566-.683.748-1.15.137-.353.3-.882.344-1.857.048-1.055.058-1.37.058-4.041v-.08c0-2.597-.01-2.917-.058-3.96-.045-.976-.207-1.505-.344-1.858a3.097 3.097 0 00-.748-1.15 3.098 3.098 0 00-1.15-.748c-.353-.137-.882-.3-1.857-.344-1.023-.047-1.351-.058-3.807-.058zM12 6.865a5.135 5.135 0 110 10.27 5.135 5.135 0 010-10.27zm0 1.802a3.333 3.333 0 100 6.666 3.333 3.333 0 000-6.666zm5.338-3.205a1.2 1.2 0 110 2.4 1.2 1.2 0 010-2.4z"
                                                clip-rule="evenodd"></path>
                                        </svg>
                                    </a>
                                </li>
                                <li>
                                    <a
                                        href="#"
                                        class="flex h-6 w-6 items-center justify-center text-gray-400 hover:text-gray-500"
                                    >
                                        <span class="sr-only">Share on X</span>
                                        <svg
                                            class="h-5 w-5"
                                            fill="currentColor"
                                            viewBox="0 0 20 20"
                                            aria-hidden="true"
                                        >
                                            <path
                                                d="M11.4678 8.77491L17.2961 2H15.915L10.8543 7.88256L6.81232 2H2.15039L8.26263 10.8955L2.15039 18H3.53159L8.87581 11.7878L13.1444 18H17.8063L11.4675 8.77491H11.4678ZM9.57608 10.9738L8.95678 10.0881L4.02925 3.03974H6.15068L10.1273 8.72795L10.7466 9.61374L15.9156 17.0075H13.7942L9.57608 10.9742V10.9738Z"
                                            ></path>
                                        </svg>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>

                    <div
                        class="hidden mx-auto mt-16 w-full max-w-2xl lg:col-span-4 lg:mt-0 lg:max-w-none"
                    >
                        <div>
                            <div class="border-b border-gray-200">
                                <div
                                    class="-mb-px flex space-x-8"
                                    aria-orientation="horizontal"
                                    role="tablist"
                                >
                                    <!-- Selected: "border-red-600 text-red-600", Not Selected: "border-transparent text-gray-700 hover:border-gray-300 hover:text-gray-800" -->
                                    <button
                                        id="tab-reviews"
                                        class="whitespace-nowrap border-b-2 border-transparent py-6 text-sm font-medium text-gray-700 hover:border-gray-300 hover:text-gray-800"
                                        aria-controls="tab-panel-reviews"
                                        role="tab"
                                        type="button"
                                        >Đánh giá từ khách hàng</button
                                    >
                                    <button
                                        id="tab-faq"
                                        class="whitespace-nowrap border-b-2 border-transparent py-6 text-sm font-medium text-gray-700 hover:border-gray-300 hover:text-gray-800"
                                        aria-controls="tab-panel-faq"
                                        role="tab"
                                        type="button">Hỏi - Đáp</button
                                    >
                                    <button
                                        id="tab-license"
                                        class="whitespace-nowrap border-b-2 border-transparent py-6 text-sm font-medium text-gray-700 hover:border-gray-300 hover:text-gray-800"
                                        aria-controls="tab-panel-license"
                                        role="tab"
                                        type="button">Tài liệu</button
                                    >
                                </div>
                            </div>

                            <!-- 'Customer Reviews' panel, show/hide based on tab state -->
                            <div
                                id="tab-panel-reviews"
                                class="-mb-10"
                                aria-labelledby="tab-reviews"
                                role="tabpanel"
                                tabindex="0"
                            >
                                <h3 class="sr-only">Customer Reviews</h3>

                                <div
                                    class="flex space-x-4 text-sm text-gray-500"
                                >
                                    <div class="flex-none py-10">
                                        <img
                                            src="https://images.unsplash.com/photo-1502685104226-ee32379fefbe?ixlib=rb-=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=8&w=256&h=256&q=80"
                                            alt=""
                                            class="h-10 w-10 rounded-full bg-gray-100"
                                        />
                                    </div>
                                    <div class="py-10">
                                        <h3 class="font-medium text-gray-900">
                                            Emily Selman
                                        </h3>
                                        <p>
                                            <time datetime="2021-07-16"
                                                >July 16, 2021</time
                                            >
                                        </p>

                                        <div class="mt-4 flex items-center">
                                            <!-- Active: "text-yellow-400", Default: "text-gray-300" -->
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
                                            >
                                                <path
                                                    fill-rule="evenodd"
                                                    d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z"
                                                    clip-rule="evenodd"></path>
                                            </svg>
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
                                            >
                                                <path
                                                    fill-rule="evenodd"
                                                    d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z"
                                                    clip-rule="evenodd"></path>
                                            </svg>
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
                                            >
                                                <path
                                                    fill-rule="evenodd"
                                                    d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z"
                                                    clip-rule="evenodd"></path>
                                            </svg>
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
                                            >
                                                <path
                                                    fill-rule="evenodd"
                                                    d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z"
                                                    clip-rule="evenodd"></path>
                                            </svg>
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
                                            >
                                                <path
                                                    fill-rule="evenodd"
                                                    d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z"
                                                    clip-rule="evenodd"></path>
                                            </svg>
                                        </div>
                                        <p class="sr-only">5 out of 5 stars</p>

                                        <div
                                            class="prose prose-sm mt-4 max-w-none text-gray-500"
                                        >
                                            <p>
                                                This icon pack is just what I
                                                need for my latest project.
                                                There's an icon for just about
                                                anything I could ever need. Love
                                                the playful look!
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div
                                    class="flex space-x-4 text-sm text-gray-500"
                                >
                                    <div class="flex-none py-10">
                                        <img
                                            src="https://images.unsplash.com/photo-1500648767791-00dcc994a43e?ixlib=rb-=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=8&w=256&h=256&q=80"
                                            alt=""
                                            class="h-10 w-10 rounded-full bg-gray-100"
                                        />
                                    </div>
                                    <div class="border-t border-gray-200 py-10">
                                        <h3 class="font-medium text-gray-900">
                                            Hector Gibbons
                                        </h3>
                                        <p>
                                            <time datetime="2021-07-12"
                                                >July 12, 2021</time
                                            >
                                        </p>

                                        <div class="mt-4 flex items-center">
                                            <!-- Active: "text-yellow-400", Default: "text-gray-300" -->
                                            <svg
                                                class="h-5 w-5 flex-shrink-0 text-yellow-400"
                                                viewBox="0 0 20 20"
                                                fill="currentColor"
                                                aria-hidden="true"
